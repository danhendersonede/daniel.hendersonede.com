<ul reversed class="card-group articlelist">
	{% for article in articleslist | reverse %}
		<li
    class="card articlelist-item{% if article.url == url %} articlelist-item-active{% endif %}"
  >
			<a href="{{ article.url }}" class="articlelist-link"
      >
				{% if article.data.title %}{{ article.data.title }}
				{% else %}
					<code
        >{{ article.url }}</code
      >
				{% endif %}
			</a
    >
			<time class="articlelist-date" datetime="{{ article.date | htmlDateString }}"
      >{{ article.date | readableDate("LLLL yyyy") }}</time
    >
		</li>
	{% endfor %}
</ul>