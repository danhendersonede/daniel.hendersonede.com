---
import linkedinIcon from '@phosphor-icons/core/assets/regular/linkedin-logo.svg';
import { Image } from 'astro:assets';
import NavLink from '@base/NavbarLink.astro';
import { pageConfig } from '@config/root-page-config';
---

{
  Object.entries(pageConfig)
    .filter(([, item]) => 'navOrder' in item)
    .sort(
      (a, b) =>
        (a[1] as { navOrder: number }).navOrder -
        (b[1] as { navOrder: number }).navOrder
    )
    .map(([, item]) => (
      <NavLink href={`/${item.slug}`}>{item.pageTitle}</NavLink>
    ))
}
<NavLink href="https://www.linkedin.com/in/danhendersonede" newTab>
  <Image src={linkedinIcon} alt="LinkedIn" loading="eager" class="icon" />
</NavLink>
